{
  "name": "rcommerz/logger-laravel",
  "description": "Production-ready structured logging for Laravel microservices with OpenTelemetry support",
  "type": "library",
  "keywords": [
    "logging",
    "structured-logging",
    "laravel",
    "microservices",
    "opentelemetry",
    "observability"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "RCOMMERZ",
      "email": "dev@rcommerz.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "illuminate/http": "^9.0|^10.0|^11.0",
    "monolog/monolog": "^3.5",
    "open-telemetry/api": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10.0",
    "orchestra/testbench": "^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Rcommerz\\Logger\\": "src/"
    },
    "files": [
      "_ide_helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Rcommerz\\Logger\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rcommerz\\Logger\\LoggerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
